{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "node_modules"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-object-literal-type-assertion": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
